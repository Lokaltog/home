snippet fun
	function ${1:function_name} (${2}) {
		${3}
	}
snippet f
	function(${1}) {${2}}
snippet if
	if (${1}){
		${2}
	}
snippet ife
	if (${1}){
		${2}
	}
	else{
		${3}
	}
snippet for
	for (var ${2:i} = 0; $2 < ${1:Things}.length; $2${3:++}) {
		${4:$1[$2]}
	};
snippet forr
	for (var ${2:i} = ${1:Things}.length - 1; $2 >= 0; $2${3:--}) {
		${4:$1[$2]}
	};
snippet wh
	while (${1:/* condition */}){
		${2:/* code */}
	}
snippet do
	do{
		${2:/* code */}
	}
	while (${1:/* condition */});
